{
  "regions": ["hnd1"],
  "crons": [
    {
      "path": "/api/cron/poll-systems",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/cron/cleanup",
      "schedule": "0 2 * * *"
    }
  ],
  "functions": {
    "app/api/cron/poll-systems/route.ts": {
      "maxDuration": 60,
      "runtime": "nodejs22.x"
    },
    "app/api/history/route.ts": {
      "maxDuration": 30,
      "runtime": "nodejs22.x"
    },
    "app/api/*/route.ts": {
      "runtime": "nodejs22.x"
    }
  }
}